{
  "name": "navify-server",
  "version": "1.0.0",
  "private": true,
  "description": "Backend for Navify",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/index.ts",
    "dev:concurrent": "concurrently \"yarn:watch\" \"yarn:dev\"",
    "lint": "eslint . --ext .ts",
    "drizzle:generate": "bun drizzle-kit generate",
    "drizzle:migrate": "bun drizzle-kit migrate",
    "db:seed": "ts-node src/db/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:address": "ts-node src/tests/test-address-creation.ts",
    "test:address:manual": "ts-node src/tests/manual-address-test.ts",
    "test:rural": "ts-node src/tests/test-rural-addressing.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ngeohash": "^0.6.3",
    "pg": "^8.15.6",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.14",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
